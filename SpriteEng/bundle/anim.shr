[BLOCK_PROGRAM]
tAtlasTex = 0

[VERTEX_PROGRAM]

void main()
{
	gl_Position = gl_ModelViewProjectionMatrix*gl_Vertex;
	gl_TexCoord[0] = gl_MultiTexCoord0;
}

[FRAGMENT_PROGRAM]

uniform sampler2D tAtlasTex;

void main()
{
	gl_FragColor = texture2D( tAtlasTex, gl_TexCoord[0].xy );
}